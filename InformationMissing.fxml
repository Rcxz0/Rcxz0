<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="566.0" prefWidth="867.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="university.missing.InformationMissingController">
   <children>
      <ImageView fitHeight="441.0" fitWidth="625.0" layoutX="122.0" layoutY="85.0" opacity="0.47" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@UOJ%20Logo.png" />
         </image>
      </ImageView>
      <Label layoutX="321.0" layoutY="66.0" text="Item information" textFill="#5b617a">
         <font>
            <Font name="Arial Bold Italic" size="34.0" />
         </font>
      </Label>
      <Label layoutX="46.0" layoutY="127.0" text="The missing thing:">
         <font>
            <Font name="System Bold" size="19.0" />
         </font>
      </Label>
      <Label layoutX="46.0" layoutY="207.0" text="Characteristics of the missing thing:">
         <font>
            <Font name="System Bold" size="19.0" />
         </font>
      </Label>
      <ComboBox fx:id="college" layoutX="47.0" layoutY="327.0" prefHeight="31.0" prefWidth="237.0" promptText="The college" />
      <TextField fx:id="name" layoutX="384.0" layoutY="127.0" promptText="name">
         <font>
            <Font size="19.0" />
         </font>
      </TextField>
      <TextArea fx:id="details" layoutX="384.0" layoutY="206.0" opacity="0.82" prefHeight="157.0" prefWidth="237.0" promptText="Details about this" />
      <Button fx:id="image" layoutX="47.0" layoutY="412.0" mnemonicParsing="false" onAction="#addImage" text="image">
         <font>
            <Font size="19.0" />
         </font>
      </Button>
      <Button fx:id="add" layoutX="454.0" layoutY="493.0" mnemonicParsing="false" onAction="#added" prefHeight="42.0" prefWidth="78.0" text="add">
         <font>
            <Font size="19.0" />
         </font>
      </Button>
      <Button fx:id="cancel" layoutX="306.0" layoutY="493.0" mnemonicParsing="false" onAction="#cancelAdd" text="cancel">
         <font>
            <Font size="19.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
